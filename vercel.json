{
  "headers": [
    {
      "source": "/sw.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        },
        {
          "key": "Service-Worker-Allowed",
          "value": "/"
        }
      ]
    }
  ],
  "env": {
    "BYPASS_AFIP_IN_PRODUCTION": "true"
  },
  "build": {
    "env": {
      "BYPASS_AFIP_IN_PRODUCTION": "true"
    }
  }
}
